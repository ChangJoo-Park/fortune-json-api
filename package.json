{
  "name": "fortune-json-api",
  "description": "JSON API serializer for Fortune.",
  "version": "1.1.16",
  "license": "MIT",
  "author": {
    "email": "0x8890@airmail.cc",
    "url": "http://0x8890.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fortunejs/fortune-json-api.git"
  },
  "bugs": "https://github.com/fortunejs/fortune-json-api/issues",
  "scripts": {
    "clean": "rimraf dist/lib dist/test",
    "build": "npm run build:lib && npm run build:test",
    "build:lib": "mkdirp dist/lib && babel lib --out-dir dist/lib",
    "build:test": "mkdirp dist/test && babel test --out-dir dist/test",
    "postpublish": "git tag `npm v fortune-json-api version` && git push origin --tags",
    "prepublish": "npm run clean && npm run build",
    "test": "npm run test:lint && npm run build && node dist/test",
    "test:lint": "eslint lib test"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "deep-equal": "^1.0.1",
    "inflection": "^1.8.0",
    "uri-templates": "^0.1.9"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "deep-equal": "^1.0.1",
    "eslint": "^1.10.3",
    "eslint-config-0x8890": "^1.0.1",
    "fortune": "^2.0.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.1"
  },
  "files": [
    "dist/lib",
    "dist/test",
    "lib/",
    "test/",
    "LICENSE"
  ],
  "main": "dist/lib/index.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "0x8890"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "keywords": [
    "json",
    "api",
    "fortune",
    "http",
    "hypermedia",
    "rest",
    "serializer"
  ]
}
